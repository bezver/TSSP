<Window x:Name="InfoKandidatenForm" x:Class="Kandidaten.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kandidaten"
        mc:Ignorable="d"
        Title="Kandidaten" Height="450" Width="800">
    <Menu x:Name="MainMenu" RenderTransformOrigin="0.5,0.5">
        <Menu.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform AngleX="-41.157"/>
                <RotateTransform/>
                <TranslateTransform X="-189.7"/>
            </TransformGroup>
        </Menu.RenderTransform>
        <MenuItem FontFamily="Calibri" Header="File">
            <MenuItem Header="Save"/>
            <MenuItem Header="Delete"/>
        </MenuItem>
        <MenuItem Header="Find">
            <MenuItem Header="Test"/>
        </MenuItem>
    </Menu>
</Window>
